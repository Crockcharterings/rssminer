#!/bin/sh

ABSOLUTE_PATH=$(readlink -f $0)
HOME="${ABSOLUTE_PATH%/*/*}"
cd "${HOME}" && java -Dsun.jnu.encoding=UTF-8 -Dfile.encoding=UTF-8 \
    -XX:+TieredCompilation -XX:+UseCompressedOops\
    -cp "classes:lib/*:src/" clojure.main \
    -e "(require 'rssminer.admin)(apply rssminer.admin/main *command-line-args*)" \
    /dev/null $@

