#!/bin/sh

ABSOLUTE_PATH=$(readlink -f $0)
HOME="${ABSOLUTE_PATH%/*/*}"
cd "${HOME}" && java \
    -Dsun.jnu.encoding=UTF-8 \
    -Dfile.encoding=UTF-8 \
    -Djava.net.preferIPv4Stack=true \
    -XX:+TieredCompilation \
    -XX:+UseCompressedOops \
    -XX:+UseCompressedStrings \
    -cp "classes:lib/*:src/" clojure.main \
    -m rssminer.admin $@

